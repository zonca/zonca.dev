   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(109, 216)"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "shape"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "284621fa-b279-4ec6-8f25-f819063400a1",
   "metadata": {
